<h1><%= @user.name %>さんのホーム</h1>

<div class= "logout_button">
  <li><%= link_to 'LOGOUT', destroy_user_session_path, method: :delete %></li>
</div>

<h2>ユーザー一覧</h2>
<div class = "user_list">
  <% if @users.nil? %>
    'anonymous'
  <% else %>
    <% @users.each do |user| %>
      <%= link_to user.name ,rooms_create_path(current_user_id: current_user.id, user_id: user.id), method: :post %>
    <% end %>
  <% end %>
</div>








